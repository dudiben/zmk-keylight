
#include <behaviors.dtsi>
#include <dt-bindings/zmk/keys.h>
#include <dt-bindings/zmk/bt.h>

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
                &bootloader
                &kp X
                &kp Y
                &kp Z
                &kp A
                &kp RGB_EFF
            >;

            sensor-bindings = <&inc_dec_kp RGB_HUI RGB_HUD &inc_dec_kp RGB_SAI RGB_SAD>;
        };
    };
};
